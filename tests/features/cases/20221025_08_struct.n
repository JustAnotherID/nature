type people = struct {
    int a
    bool b
    int c
    [int;5] list
    [f32] flist
}

fn main() {
    var foo = people {
        a = 22,
        b = true,
    }

    foo.c = 11
    foo.list = [1, 2, 3, 4, 5]

    println('foo.a =', foo.a)
    println('foo.b =', foo.b)
    println('foo.c =', foo.c)
    println('foo.list[0] =', foo.list[0])
    println('foo.list[1] =', foo.list[1])
    println('foo.list[2] =', foo.list[2])
    println('foo.list[3] =', foo.list[3])
    println('foo.list[4] =', foo.list[4])

    foo.flist = [3.55, 3.625]

    println(foo.flist[0])
    println(foo.flist[1])
}
