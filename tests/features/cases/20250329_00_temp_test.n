fn dump(f32 amount) {
    println(amount)
}

type atm_t = struct {
    string name
    string address
    f32 amount
    int count
    string bank
}

fn main() {
    [atm_t] list = vec_new(atm_t{}, 8)

    list[0] = atm_t{
        amount = 100,
    }

    list[3] = atm_t{
        amount = 400,
    }

    list[5] = atm_t{
        amount = 600,
    }

    list[7] = atm_t{
        amount = 800,
    }

    for int i = 0; i < 8; i += 1 {
        dump(list[i].amount)
    }
}